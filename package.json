{
  "packageManager": "bun@0.1.10",
  "name": "arach-io",
  "version": "4.2.0",
  "private": false,
  "scripts": {
    "dev": "astro dev --experimental-https",
    "start": "astro dev",
    "build": "astro build && jampack ./dist",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check . --plugin=prettier-plugin-astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "cz": "cz",
    "prepare": "husky install",
    "lint": "eslint",
    "postinstall": "node scripts/install-sharp.js"
  },
  "dependencies": {
    "@astrojs/check": "^0.4.1",
    "@astrojs/partytown": "^2.1.0",
    "@astrojs/rss": "^4.0.6",
    "@divriots/jampack": "0.23.4",
    "@fontsource-variable/dosis": "^5.0.20",
    "@fontsource/inconsolata": "^5.0.18",
    "@fontsource/inter": "^5.2.6",
    "@fontsource/share-tech-mono": "^5.0.12",
    "@resvg/resvg-js": "^2.6.2",
    "@tailwindcss/typography": "^0.5.13",
    "astro": "^4.8.4",
    "astrojs": "^0.1.3",
    "fuse.js": "^7.0.0",
    "geist": "^1.3.0",
    "github-slugger": "^2.0.0",
    "mdast-util-to-string": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-rating": "^2.0.5",
    "reading-time": "^1.5.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.10.13",
    "sharp": "^0.33.4",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@astrojs/react": "^3.3.4",
    "@astrojs/sitemap": "^3.1.4",
    "@astrojs/tailwind": "^5.1.0",
    "@types/github-slugger": "^1.3.0",
    "@types/react": "^18.3.2",
    "@typescript-eslint/parser": "^6.21.0",
    "astro-eslint-parser": "^0.16.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.31.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "node-gyp": "^9.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.14"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json,astro}": [
      "prettier --write --plugin=prettier-plugin-astro"
    ]
  }
}
